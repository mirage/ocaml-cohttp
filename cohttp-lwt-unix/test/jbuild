(jbuild_version 1)

(executables
 ((libraries (cohttp-lwt-unix oUnit lwt.unix))
  (modules (test_parser))
  (names (test_parser))))

(executables
 ((libraries (cohttp_lwt_unix_test cohttp-lwt-unix logs))
  (modules (test_sanity))
  (names (test_sanity))))

(alias
 ((name   runtest)
  (package cohttp-lwt-unix)
  (deps   (test_parser.exe))
  (action (run ${<}))))

(alias
 ((name   runtest)
  (package cohttp-lwt-unix)
  (deps   (test_sanity.exe))
  (action (run ${<}))))
